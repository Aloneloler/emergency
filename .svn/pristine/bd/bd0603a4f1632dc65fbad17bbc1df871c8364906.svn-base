.itemList{
  margin-bottom:10px;
  padding: 5px 10px;
  border:1px solid;
  //border-bottom: 1px solid;
  //border-bottom-color: #00b8ff;
  overflow: hidden;
  display: flex;
  align-items: center;
  border-radius: 5px;
}
.listIcon{
  width: auto;
  float: left;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;

  .leftTxt{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    box-sizing:unset;
    //text-align: left;
    -moz-box-sizing: unset; /*Firefox3.5+*/
    -webkit-box-sizing: unset; /*Safari3.2+*/
    -o-box-sizing:unset; /*Opera9.6*/
    -ms-box-sizing: unset; /*IE8*/
  }
}
