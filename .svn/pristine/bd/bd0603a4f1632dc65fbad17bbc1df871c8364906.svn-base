.reactMarqueeBox{
  width:100%;
  height:100%;
  //position: absolute;
  //top:0;
  //left:0;
  //overflow: hidden;
  font-family: "Microsoft YaHei";
}
.headLine{
  width: 100%;
  //background: #045881;
  text-align: left;
  color: #fff;
}
.reactMarqueeBox .marqueeVerticalWrap{
  width:100%;
  height:100%;
  overflow: hidden;
}

.reactMarqueeBox .marqueeVerticalWrap .marqueeVerticalItem .marqueeVerticalTxt{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  box-sizing:unset;
  text-align: left;
  -moz-box-sizing: unset; /*Firefox3.5+*/
  -webkit-box-sizing: unset; /*Safari3.2+*/
  -o-box-sizing:unset; /*Opera9.6*/
  -ms-box-sizing: unset; /*IE8*/
  //border-bottom: 3px solid #00b8ff;
  transition: all .3s ease;
  margin-top: 10px;

}
.marqueeVerticalTitle{
  transition: all .3s ease;
  //margin-bottom: 10px;
}
.reactMarqueeBox .marqueeVerticalWrap .marqueeVerticalItem .marqueeVerticalTxt span{
  display: inline-block;
  margin-right: 10px;
}

