.menuBox{
    width:15%;
    height:100%;
    background:rgba(255,255,255,1);
    border:1px solid rgba(239,240,241,1);
    .search{
        margin: 32px auto;
        text-align: center;

        .inputSearch{
            width: 80%;
            margin: 0 auto;
            height: 41px;
            line-height: 41px;
            
        }
        :global {
            .ant-input-affix-wrapper .ant-input:not(:first-child) {
                padding-left: 30px;
                border-radius: 20px;
            }
        }
    }
    
    .departmentBox{
        font-weight: 600;
        font-family: AlibabaPuHuiTiR;
        img{
            margin-right: 10px;
        }
    }
}
.operation{
    text-align: center;
    .operationDelete{
        margin: 0 15px;
    }
    .operationShare{
        margin: 0 7.5px;
    }
    img{
        cursor: pointer;
    }
    
}