.allBoxs {
    // width: calc(100% - 100px);
    width: 100%;
    padding-left: 74px;
    overflow: hidden;
    height: 100%;
    // float: right;
  }
  
  .leftBox {
    width: 6%;
    height: 100%;
    background: rgba(30, 30, 45, 1);
  }
  
  .rightBox {
    width: 100%;
    height: 100%;
    float: right;
    display: flex;
    flex-wrap: wrap;
  
  }
  
  
  ::-webkit-scrollbar {
    width: 8px;
    background-color: #fff;
  }
  
  ::-webkit-scrollbar-thumb {
    background-color: #DDDFE1;
    border-radius: 4px;
  }
  
  .ContentBlock {
    display: flex;
    width: 85%;
    height: 100%;
    flex-wrap: wrap;
  }
  .operation{
    text-align: center;
    .operationDelete{
        margin: 0 15px;
    }
    img{
        cursor: pointer;
    }
}