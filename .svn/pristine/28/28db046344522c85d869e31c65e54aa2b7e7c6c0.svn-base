.contentBox{
    width: 80%;
    height: 60%;
    // min-width: 680px;
    background: rgba(246,248,249,1);
    overflow-x: hidden;
    overflow-y: scroll;
    padding: 10px 20px;
    display: flex;
    flex-wrap: wrap;
    align-content:flex-start;

    
    .callCard{
        width:30%;
        min-width: 290px;
        height: 30%;
        background:rgba(255,255,255,1);
        box-shadow:0px 0px 6px 0px rgba(0, 0, 0, 0.11);
        margin: 1%;
        position: relative;
        font-family: MicrosoftYaHei;

        .x{
            position: absolute;
            top: 8%;
            right: 5%;
            cursor: pointer;
        }
        .callTop{
            display: flex;
            margin-top: 10%;
            .callLeft{
                width: 30%;
                margin: 0 auto;
                display: flex;
                justify-content: center;
                flex-direction: column;
                align-items: center;
                .callState{
                    margin-top: 10px;
                }
                .onCall{
                    color:'#3389FF'
                }
                .Suspended{
                    color: #666666;
                }
            }
            .callRight{
                width: 60%;
                .userTitle{
                    color:#666666;
                }
            }
        }
        .callFooter{
            // display: flex;
            // justify-content: space-around;
            // align-items: center;
            padding-top: 5%;

            .callFooterLeft{
                display: inline-flex;
                width: 70%;
                float: left;
                padding-left: 10%;
                color: #666666;
                div{
                    padding: 0 5px;

                    img{
                        padding: 0 3px;
                    }
                }
                
            }
            .callBtn{
                // width: 30%;
                float: right;
                padding: 0 13px;
                margin-right:22px;
            }
        }
    }
}
.callModel{
    background: url('@/assets/VoiceDispatching/bg.png');
    box-shadow:0px 0px 6px 0px rgba(0, 0, 0, 0.11);
    border-radius: 8px;
    width: 20%;
    height: 50%;
    position: absolute;
    top: 30%;
    z-index: 2;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
    color: #A7ABB1;

    .modelTop{
        margin-top: 20%;
    }
    .modelContent{
        h3{
            color: #FFFFFF;
            margin: 18px;
            font-size: 18px;
        }
        font-size: 16px;

    }
    .modelControl{
        margin-top: 20%;
        display: flex;
        justify-content: center;
       
        .modelControlBtn{
            margin: 0 30px;
            
            .modelControlImg{
                cursor: pointer;
                background-color: #FFFFFF;
                width: 40px;
                height: 40px;
                border-radius: 50%;
                line-height: 40px;
                margin-bottom: 8px;
            }
           

        }
    }
    .modelFooter{
        margin-top: 10%;
        img{
            margin: 0 30px;
            cursor: pointer;
        }
    }
}

.popup{
    position: absolute;
    bottom: 16%;
    right: 2%;
    width: 269px;
    height: 251px;
    background: rgba(0, 0, 0, 0.8);
    z-index: 3;
  
    .popupTop{
      display: flex;
      align-items: center;
      height: 30%;
      color: #fff;
      position: relative;
  
      .x{
        position: absolute;
        top: 16px;
        right: 16px;
      }
  
      .user{
        margin: 0 20px;
      }
    }
  
    .popupContent{
      display: flex;
      justify-content: center;
      margin-bottom: 20px;
    }
  
    .popupFooter{
      display: flex;
      justify-content: center;
  
      .popupBtn{
        margin-right: 20px;
        background-color: #19AB19;
        border-color: #19AB19;
      }
      .closeBtn{
        background-color: #F35353;
        border-color: #F35353;
      }
    }
  }
  