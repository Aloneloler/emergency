.allBox {
  width: calc(100% - 74px);
  height: 100%;
  float: right;
  background-color: #ffffff;

  .inputBox {
    margin-top: 3%;
    width: 100%;
    height: 10%;
    display: flex;
    align-items: center;
    padding: 0 60px;
    background-color: #ffffff;

    div {
      width: 25%;

      input {
        width: 70%;
      }

      .seachBtn {
        margin-right: 20px;
      }
    }
  }

  .tableBox {
    width: 100%;
    height: 90%;
    padding: 0 45px;
    background-color: #ffffff;
  }
}
.modalInput{
  display: flex;
  align-items: center;
  margin: 10px auto;

  .modalTitle{
    margin-right: 10px;
    width: 20%;
    text-align: right;
  }
  
}
:global {
  input[type='text'], input[type='password'], input[type='number'], textarea {
      width: 50%;
  }
  textarea.ant-input {
    max-width: 50%;
  }
}
